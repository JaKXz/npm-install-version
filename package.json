{
  "name": "npm-install-version",
  "version": "5.1.0",
  "description": "Installs node modules to versioned or custom directories.",
  "author": "Scott Hardy",
  "license": "MIT",
  "repository": "git@github.com:scott113341/npm-install-version.git",
  "homepage": "https://github.com/scott113341/npm-install-version",
  "bugs": "https://github.com/scott113341/npm-install-version/issues",
  "keywords": [
    "npm",
    "install",
    "multiple",
    "versions"
  ],
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "bin": {
    "niv": "lib/cli.js",
    "npm-install-version": "lib/cli.js"
  },
  "scripts": {
    "build": "shx rm -rf lib/ && shx mkdir lib/ && babel --copy-files --out-dir lib/ src/",
    "lint": "semistandard --fix",
    "test": "npm run build && node lib/test/index.js",
    "test:ci": "node lib/test/index.js && semistandard"
  },
  "dependencies": {
    "deasync": "^0.1.7",
    "minimist": "^1.2.0",
    "npm": "^3.10.5",
    "sanitize-filename": "^1.6.0",
    "shelljs": "^0.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "np": "^2.9.0",
    "semistandard": "^9.0.0",
    "shx": "^0.2.0",
    "tape": "^4.6.0"
  },
  "semistandard": {
    "ignore": [
      "/lib/"
    ]
  }
}
